import sys

def factorize(nb):
    fct = []
    for x in range(2, int(nb**0.5) + 1):
        while nb % x == 0:
            fct.append(x)
            nb //= x
    if nb > 1:
        fct.append(nb)
    return (fct)

try:
    for line in sys.stdin:
        nb = int(line.strip())
        fcts = factorize(nb)
        fcts_str = "*".join(map(str, fcts))
        print(f"{nb}={fcts_str}")
except KeyboardInterrupt:
    pass
